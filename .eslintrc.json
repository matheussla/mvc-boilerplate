{
  "env": {
      "browser": false,
      "es6": true,
      "mocha": true
  },
  "extends": [
      "airbnb-base"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "rules": {
      "max-len": [2, 120, 4, {"ignoreUrls": true}],
      "no-restricted-syntax": 0,
      "no-await-in-loop": 0,
      "linebreak-style": 0
  }
}
